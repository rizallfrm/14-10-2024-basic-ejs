<table class="table table-striped">
    <thead>
        <tr>
            <th>name</th>
            <th>photo profile</th>
            <th>age</th>
            <th>phone number</th>
        </tr>
    </thead>
    <tbody>
        <% users.map((user)=> { %>
            <tr>
                <td>
                    <%= user.firstName %>
                </td>
                <td>
                    <%= user.lastName %>
                </td>
                <td>
                    <%= user.age %>
                </td>
                <td>
                    <%= user.photoProfile %>
                </td>
                <td>
                    <%= user.phoneNumber %>
                </td>
                <td>
                    <img src="<%= user.photoProfile %>" class="rounded mx-auto d-block img-fluid">
                </td>

            </tr>
            <%})%>
    </tbody>

</table>